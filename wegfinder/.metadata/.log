     */
    private void refreshFloorTable(DefaultTableModel tableModel) {
        tableModel.setRowCount(0); // Clear existing data
        List<Object[]> floors = adminCal.getAllFloors();
        for (Object[] floor : floors) {
            tableModel.addRow(floor);
        }
    }

